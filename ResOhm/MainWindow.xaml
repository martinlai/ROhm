<Window x:Class="ResOhm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResOhm"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="10,0,-10,-21">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="515*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>

        <StackPanel Margin="50,77,50,100">
            <ComboBox Height="34" RenderTransformOrigin="0.5,0.5" >
                <ComboBoxItem IsSelected="True" Content="3 Bands"/>
                <ComboBoxItem Content="4 Bands"/>
                <ComboBoxItem Content="5 Bands"/>
                <ComboBoxItem Content="6 Bands"/>
            </ComboBox>

        </StackPanel>
        <Button x:Name="button" Content="Confirm" HorizontalAlignment="Center" Margin="210,200,210,0" VerticalAlignment="Top" Width="75"/>



    </Grid>
</Window>
